<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Blog Ninja | Home</title>
    <link rel="stylesheet" href="/styles.css">
</head>

<body>
    <%- include("./partials/nav.ejs")%>

   

    <div class="blogs content">
        <h2>All Blogs</h2>
        <% blogs.forEach(blog => {%>
            <h3><%= blog.title %></h3>
            <p><%= blog.content %></p>
        <% })%>
    </div>
</body>

</html>